<vertical vertical-fit='MinSize' min-height='80' xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' xsi:schemaLocation='https://monkeymanboy.github.io/BSML-Docs/ https://raw.githubusercontent.com/monkeymanboy/BSML-Docs/gh-pages/BSMLSchema.xsd'>
	<horizontal anchor-min-y='1'  bg='panel-top' pad-left='10' pad-right='10' horizontal-fit='PreferredSize'>
		<text text='Changelog' italics='true' align='Center' font-size='8' />
	</horizontal>
	<horizontal/>
	<loading-indicator active='~is-loading' preserve-aspect='true' pref-width='20' pref-height='20' />
	<horizontal/>
	<horizontal active='~loaded' horizontal-fit='PreferredSize' vertical-fit='PreferredSize' pref-width='110' pref-height='60' spacing='2'>
		<text-page id='text-page' text='~changelog' rich-text='true' italics='true' /> <!--Text is coming from a reliable source (me)-->
	</horizontal>
</vertical>